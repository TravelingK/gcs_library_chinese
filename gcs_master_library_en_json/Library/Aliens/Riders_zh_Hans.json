                    "children": {
                        "a6e87ff7-9fc6-4ebe-a81a-906230b59ea6": {
                            "name": "伤害耐受",
                            "tags": [
                                "优势",
                                "异种",
                                "生理"
                            ],
                            "notes": "Rider are \"Diffuse\" but the host in NOT.",
                            "modifiers": {
                                "c5eb187f-1cab-48c4-8e36-a6fcae416b8b": {
                                    "name": "弥散",
                                    "notes": "免疫致残损伤。无法瞄准大脑、要害、胯下。大部分敌人（依 GM 决定）不能撞到或抓住你。不会流血。不会被血源性毒物影响。免疫任何通过切断你身体部分血液供应造成伤害的攻击。穿刺和贯通攻击（任何程度的）无法对它们造成比 1 HP 更严重的损伤，无论是否穿透。其他种类的攻击无法造成比 2 HP 更严重的损伤。例外：有范围效果、锥形、爆炸的攻击能正常对它们造成正常伤害"
                                },
                                "48bebd3b-33ea-4f23-8cbe-c9217119a542": {
                                    "name": "均质",
                                    "notes": "更改伤害修正因子：穿刺和巨型贯通的创伤修正为 ×1/2，大型贯通 ×1/3，中型贯通 ×1/5，小型贯通则 ×1/10"
                                },
                                "5a29f15b-101e-405c-8d19-d7c935519659": {
                                    "name": "无血",
                                    "notes": "不会流血，不会被血源性毒物影响，并且对任何通过切断你身体部分血液供应造 成伤害的攻击免疫"
                                },
                                "bcf9a4b0-4b05-4592-9855-8bb9c752edd6": {
                                    "name": "无脑",
                                    "notes": "无法瞄准大脑，但对颅脑或眼睛造成的打击等同于打在脸上（除非此打击可以直接瘫痪你的眼部）"
                                },
                                "a92ad1ec-b0c4-4477-a253-1e0e112c76bd": {
                                    "name": "无眼",
                                    "notes": "不能被攻击眼睛。免疫致盲攻击"
                                },
                                "c575342e-9467-45ce-931d-b25eee09d63d": {
                                    "name": "无头",
                                    "notes": "“颅脑”与“脸部”不能作为攻击目标"
                                },
                                "0488754c-8df3-4de9-a261-9972d0e964d1": {
                                    "name": "无颈",
                                    "notes": "脖子不能够作为打击的目标，不能被斩首，呛到或绞杀"
                                },
                                "a43d9ead-bd08-4297-abbd-30b7a132791c": {
                                    "name": "无要害",
                                    "notes": "对“要害”或“胯下”的攻击视为对躯干的攻击"
                                },
                                "174329bc-4570-40cc-8ca1-7ac5f55f9f25": {
                                    "name": "无生命",
                                    "notes": "更改伤害修正因子：穿刺与巨型贯通的创伤修正为 ×1，大型贯通 ×1/2，中型贯通 ×1/3，小型贯通则 ×1/5"
                                }
                            }
                        },
